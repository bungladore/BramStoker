package com.bramstoker.controller.events
{
	import flash.events.Event;
	
	public class SoundEvent extends Event
	{
		private static const NAME:String = "Sound";
		public static const MAKE_SOUND:String = NAME + "MAKE_SOUND";
		
		public function SoundEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false)
		{
			super(type, bubbles, cancelable);
		}
	}
}