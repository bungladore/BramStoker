package mrflvhead.parts{		import flash.display.Sprite;	import flash.text.TextField;	import flash.text.TextFieldAutoSize;	import flash.text.TextFormat;		public class MetaDataDisplay extends Sprite	{		private var _meta_text:TextField;				public function MetaDataDisplay()		{			super();						_meta_text = new TextField();            _meta_text.autoSize = TextFieldAutoSize.RIGHT;            _meta_text.background = true;            _meta_text.border = false;			var format:TextFormat = new TextFormat();            format.font = "Verdana";            format.color = 0x000000;            format.size = 10;            format.underline = true;            _meta_text.defaultTextFormat = format;            addChild(_meta_text);		}				public function updateDisplay(mdata:Object):void		{			trace("creator " + mdata.creator);			trace("metadatacreator " + mdata.metadatacreator);			trace("hasKeyframes " + mdata.creator);			trace("hasVideo " + mdata.hasVideo);			trace("hasAudio " + mdata.hasAudio);			trace("hasMetaData " + mdata.hasMetaData);			trace("canSeekToEnd " + mdata.canSeekToEnd);			trace("duration " + mdata.duration);			trace("datasize " + mdata.datasize);			trace("videosize " + mdata.videosize);			trace("videocodecid " + mdata.videocodecid);			trace("audiosize " + mdata.audiosize);			trace("audiocodecid " + mdata.audiocodecid);			trace("audiosamplerate " + mdata.audiosamplerate);			trace("audiosamplesize " + mdata.audiosamplesize);			trace("stereo " + mdata.stereo);			trace("filesize " + mdata.filesize);			trace("lasttimestamp " + mdata.lasttimestamp);			trace("lastkeyframetimestamp " + mdata.lastkeyframetimestamp);			trace("lastkeyframelocation " + mdata.lastkeyframelocation);			/*trace("keyframes (filepositions, times) " + mdata.keyframes(filepositions, times));*/			trace("width " + mdata.width);			trace("height " + mdata.height);			trace("framerate " + mdata.framerate);			trace("videodatarate " + mdata.videodatarate);			trace("audiodatarate " + mdata.audiodatarate);		}	}}