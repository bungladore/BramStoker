package mrflvhead.parts{		import flash.display.Sprite;	import flash.events.MouseEvent;	import flash.display.Bitmap;	import flash.events.Event;		public class MuteButton extends Sprite	{				private var _toggle_me:Boolean= true;		private var _btn_container:Sprite;		private var _mute_btn:Bitmap;		private var _unmute_btn:Bitmap;				public function MuteButton(mute_btn:Bitmap, unmute_btn:Bitmap)		{			super();						_mute_btn = mute_btn;			_unmute_btn = unmute_btn;			_btn_container = new Sprite();			_btn_container.addChild(_unmute_btn);			_btn_container.addChild(_mute_btn);			addChild(_btn_container);			_btn_container.addEventListener(MouseEvent.CLICK, muteToggle, false, 0, true);			_btn_container.buttonMode = true;		}				private function muteToggle(e:MouseEvent):void		{			if(_toggle_me){				mute();			}else{				unmute();			}		}				public function mute():void		{			dispatchEvent(new Event("muteVideo"));			_unmute_btn.visible = true;			_mute_btn.visible = false;			_toggle_me = false;		}				public function unmute():void		{			dispatchEvent(new Event("unmuteVideo"));			_unmute_btn.visible = false;			_mute_btn.visible = true;			_toggle_me = true;		}	}}